static List<Integer> compareTriplets(List<Integer> a, List<Integer> b) {
        int alice = 0;
        int bob = 0;
        List<Integer> res = new ArrayList();

        for (int i=0 ; i < 3; i++ ){
         if (a.get(i) > b.get(i))
            alice++;

         if (a.get(i) < b.get(i))
            bob++;

        }
        res.add(alice);
        res.add(bob);
        return res;
    }
